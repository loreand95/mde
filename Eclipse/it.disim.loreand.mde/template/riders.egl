<html>
	<head>
		<meta charset="UTF-8">
		<meta name="author" content="[%=cp.manager%]">
		<title> [%=cp.name%] </title>
		
		<style>
			td { width:100px }
			table, th, td {
  				border: 1px solid black;
			}
		</style>
	</head>
	<body>
		<a href="index.html"><h1>[%=cp.name%]</h1></a>
		<hr>
		<h2>Riders</h2>
		<table>
			<tr>
				<th>ID</th>
				<th>Name</th>
				<th>Surname</th>
				<th>Status Day</th>
				<th>Live Position</th>
				<th>Routes</th>
			</tr>
			[%for (rider in cp.riders) { %]
			<tr>
				<td>
					[%=rider.id%]
				</td>
				<td>
					[%=rider.name%]
				</td>
				<td>
					[%=rider.surname%]
				</td>
				<td>
					[%=rider.status%]
				</td>
				<td>
					position
				</td>
				<td>
					<a href="routes.html"> View</a>
				</td>
			</tr>
			[%}%]
		</table>
	</body>
</html>